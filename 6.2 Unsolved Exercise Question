// 6.2 Unsolved Question

#include<iostream>
using namespace std;
void reverse(int arr[], int n){
    int sum=0;
    for(int i=1;i<=n;i++){
        sum=sum+i;
        cout<<" "<<i<<"\t  "<<sum<<"\n";
    }
    cout<<"\n";
}
int main(){
    system("CLS");
    cout<<" - - - - - - - -\n";
    cout<<"Num \t Sum\n";
    cout<<" - - - - - - - -\n";
    int arr[5];
    reverse(arr,5);
    return 0;
}
